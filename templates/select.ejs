            <label for="email">Email Address: </label>
            <input type="email" id="email" name="email">
        </fieldset>
        <input type="reset" value="Reset">
        <input type="submit" value="Submit Request">
    </form>

    <hr>
    <a href="/">HOME</a>

</body>

</html>
